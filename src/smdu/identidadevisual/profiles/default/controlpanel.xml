<?xml version="1.0"?>
<object name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="smdu.identidadevisual"
    purge="False">
  <configlet title="Visual Identity"
      action_id="visualidentity"
      appId="smdu.identidadevisual"
      category="Products"
      condition_expr=""
      url_expr="string:${portal_url}/@@visualidentity-settings"
      icon_expr="string:${portal_url}/++resource++smdu.identidadevisual/icon.png"
      visible="True"
      i18n:attributes="title">
    <permission>smdu.identidadevisual: Setup</permission>
  </configlet>
</object>
